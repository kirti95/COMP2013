<Window x:Class="BasicKinect_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525" Closing="Window_Closing">
    <Grid>
        <Viewbox HorizontalAlignment="Left" Height="309" VerticalAlignment="Top" Width="256">
            <Image Height="107" Width="85" Stretch="UniformToFill" Source="{Binding ImageSource}"/>
        </Viewbox>
        <Viewbox HorizontalAlignment="Left" Height="309" Margin="261,0,0,0" VerticalAlignment="Top" Width="256">
            <Image Height="126" Width="100" Source="{Binding ImageSourceTwo}" Stretch="UniformToFill"/>
        </Viewbox>

    </Grid>
</Window>
