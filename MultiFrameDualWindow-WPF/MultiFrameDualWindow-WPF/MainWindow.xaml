<Window x:Class="MultiFrameDualWindow_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525" Closing="Window_Closing">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Viewbox HorizontalAlignment="Left" Height="299" Margin="10,10,0,0" VerticalAlignment="Top" Width="238">
            <Image Height="127" Width="100" Source="{Binding ImageSource}"/>
        </Viewbox>
        <Viewbox Grid.Column="1" HorizontalAlignment="Left" Height="299" Margin="10,10,0,0" VerticalAlignment="Top" Width="239">
            <Image Height="126" Width="100" Source="{Binding ImageSourceTwo}"/>
        </Viewbox>

    </Grid>
</Window>
